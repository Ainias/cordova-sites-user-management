{
  "name": "cordova-sites-user-management",
  "displayName": "Easy Sync",
  "version": "0.1.0",
  "description": "Ein Usermanagement für Cordova-Sites",
  "scripts": {
    "build": "node bin/build.js & tsc && cd ./src && find . -regextype sed -regex '.*\\.\\(html\\|png\\|svg\\|json\\)' -type f -exec cp --parents {} ../dist \\;",
    "update packages": "node bin/updateCopies.js"
  },
  "author": "Silas Günther",
  "peerDependencies": {
    "cordova-sites": "git+https://github.com/Ainias/cordova-sites#0.6.4",
    "cordova-sites-easy-sync": "git+https://github.com/Ainias/cordova-sites-easy-sync#0.6.6",
    "cordova-sites-database": "git+https://github.com/Ainias/cordova-sites-database#0.4.4",
    "cs-event-manager": "git+https://github.com/Ainias/event-manager.git#0.2",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.2.17",
    "nodemailer": "^6.2.1",
    "js-helper": "git+https://github.com/Ainias/js-helper#0.3.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.50",
    "cordova-sites": "git+https://github.com/Ainias/cordova-sites.git#0.6.4",
    "cordova-sites-database": "git+https://github.com/Ainias/cordova-sites-database.git#0.4.4",
    "cordova-sites-easy-sync": "git+https://github.com/Ainias/cordova-sites-easy-sync.git#0.6.6",
    "cs-event-manager": "git+https://github.com/Ainias/event-manager.git#0.2",
    "express": "^4.16.4",
    "js-helper": "git+https://github.com/Ainias/js-helper.git#0.3.3",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.10",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.6"
  }
}
